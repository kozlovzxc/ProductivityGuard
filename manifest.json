{
  "manifest_version": 2,

  "name": "ProductivityGuard",
  "description": "This extension will block very often visited unwanted sites",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
	"tabs",
    "storage"
  ],
  "background": {
    "scripts": [
        "jquery-3.1.1.js",
        "utils.js",
        "background.js"
    ],
    "persistent": false
  }
}
